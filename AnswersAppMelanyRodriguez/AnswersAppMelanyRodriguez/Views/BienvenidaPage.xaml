<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AnswersAppMelanyRodriguez.Views.BienvenidaPage">
    <ContentPage.Content>

        <StackLayout VerticalOptions="Center" Padding="50,25,50,25">
            <Button x:Name="BtnIngresar" Text="Ingresar" BackgroundColor="Gold" CornerRadius="15" Clicked="BtnIngresar_Clicked" />
            <Button x:Name="BtnAddAsk" Text="Agregar" BackgroundColor="Gold" CornerRadius="15" Clicked="BtnAddAsk_Clicked"/>
            
            <Button x:Name="BtnAskList" Text="AskList" BackgroundColor="Gold" CornerRadius="15" Clicked="BtnAskList_Clicked" />
        </StackLayout>
        
    </ContentPage.Content>
</ContentPage>